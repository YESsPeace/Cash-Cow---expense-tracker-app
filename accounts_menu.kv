<AccountsMenu>:
    BoxLayout:
        orientation: 'vertical'

        MDTopAppBar:
            id: top_bar

            elevation: 4
            pos_hint: {"top": 1}

            MDIconButton:
                icon: 'menu'
                on_release: nav_drawer.set_state("open")

        AccountsMenu_main:
            id: AccountsMenu_main_id

    MyNavigationDrawer:
        id: nav_drawer